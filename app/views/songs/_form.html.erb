<%= form_with model: @song, local: true %>
  <div class='form_field'>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <br>

  <div class='form_field'>
    <%= f.label :artist_name %>
    <%= f.text_field :artist_name %>
  </div>

  <br>

  <div class='form_field'>
    <%= f.label :genre %>
    <%= f.text_field :genre %>
  </div>

  <br>

  <div class='form_field'>
    <%= f.label :released %>
    <%= f.check_box :released %>
  </div>

  <br>

  <div class='form_field'>
    <%= f.label :release_year %>
    <%= f.date_field :release_year %>
  </div>

  <br>

  <%= f.submit %>
<% end %>
